   ],
   "source": [
    "using SymPy\n",
    "@vars x1 x2\n",
    "f = x1*sin(x2) + 1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "\\begin{equation*}\\left[\\begin{matrix}\\sin{\\left(x_{2} \\right)}\\\\x_{1} \\cos{\\left(x_{2} \\right)}\\end{matrix}\\right]\\end{equation*}"
      ],
      "text/plain": [
       "⎡ sin(x₂)  ⎤\n",
       "⎢          ⎥\n",
       "⎣x₁⋅cos(x₂)⎦"
      ]
     },
     "execution_count": 6,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "∇f = diff(f, [x1, x2])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "\\begin{equation*}\\left[\\begin{matrix}0\\\\2\\end{matrix}\\right]\\end{equation*}"
      ],
      "text/plain": [
       "⎡0⎤\n",
       "⎢ ⎥\n",
       "⎣2⎦"
      ]
     },
     "execution_count": 7,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "subs(∇f, (x1, 2), (x2, 0))"
   ]
